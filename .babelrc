{
  "ignore": [
    "node_modules"
  ],
  "presets": [
    "react",
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "safari >= 9"],
        "node": "8"
      }
    }]
  ],
  "plugins": [
    ["styled-jsx/babel",{ "plugins": ["styled-jsx-plugin-sass"] }],
    [
      "transform-react-remove-prop-types",
      {
        "mode": "wrap"
      }
    ]
  ],
  "env": {
    "dev": {
      "presets": [
        "es2015",
        "react-hmre"
      ]
    },

    "ghPages": {
      "presets": [
        "es2015"
      ]
    },
    "modules": {
      "presets": [
        [
          "es2015",
          {
            "modules": "umd"
          }
        ]
      ]
    },
    "es6": {
      "presets": [
        [
          "env",
          {
            "modules": false
          }
        ]
      ]
    },
    "test": {
      "presets": [
        "es2015",
        "react"
      ]
    }
  }
}
